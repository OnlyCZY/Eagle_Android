<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_reveiver_push"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:id="@+id/push_alarm_info"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:background="@color/stripListViewGrayColor"
        android:gravity="center_vertical"
        android:text="信息"/>

    <TextView
        android:id="@+id/push_alarm_status"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:gravity="center_vertical"
        android:text="信息"/>

    <TextView
        android:id="@+id/push_alarm_level"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:background="@color/stripListViewGrayColor"
        android:gravity="center_vertical"
        android:text="信息"/>

    <TextView
        android:id="@+id/push_alarm_time"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:gravity="center_vertical"
        android:text="信息"/>

    <TextView
        android:id="@+id/push_alarm_finish_time"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:background="@color/stripListViewGrayColor"
        android:gravity="center_vertical"
        android:text="信息"/>

    <TextView
        android:id="@+id/push_alarm_user"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:gravity="center_vertical"
        android:text="信息"/>

    <TextView
        android:id="@+id/push_alarm_confirm_time"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:background="@color/stripListViewGrayColor"
        android:gravity="center_vertical"
        android:text="信息"/>
</LinearLayout>